{
  "name": "pulsar",
  "version": "0.0.3",
  "scripts": {
    "dev": "concurrently \"cross-env BROWSER=none PORT=5000 react-scripts start\" \"electron .\"",
    "format": "prettier --write \"**/*.+(js|json|css)\"",
    "build-react": "react-scripts build",
    "build-electron": "electron-packager . --out=dist --overwrite",
    "build": "yarn build-react && yarn build-electron",
    "dist": "yarn build-react && electron-packager . --out=dist --overwrite --platform=linux,win32,darwin",
    "zip-dist": "./zip-dist.sh"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.3",
    "electron-packager": "^15.1.0",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.3"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.20.0",
    "electron-is-dev": "^1.2.0",
    "env-paths": "^2.2.0",
    "leven": "^3.1.0",
    "mathjs": "^7.5.1",
    "moment": "^2.29.1",
    "randomstring": "^1.1.5",
    "react-twemoji": "^0.3.0",
    "xdg-apps": "^1.1.0"
  },
  "homepage": "./",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
